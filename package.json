{
  "name": "rbp-frontend",
  "version": "1.0.0",
  "description": "Frontend for the Really Bad Players gaming platform.",
  "author": "John Gilbert",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts --port 8000",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start --port 8000",
    "generate": "nuxt-ts generate",
    "lint": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
