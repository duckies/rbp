{
  "name": "rbp-backend",
  "version": "0.0.1",
  "description": "API backend for the RBP gaming community.",
  "author": "Duckie",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"**/*.{ts,js,json,md,yml}\" --ignore-path .gitignore --write",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev-cli": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:prod": "node dist/main",
    "test": "jest --ci --color",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env CONFIG_PATH=../testing.env jest --config ./jest-e2e.json --ci --color -i"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/bull": "^0.1.1",
    "@nestjs/cli": "^7.1.4",
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.9",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/platform-socket.io": "^7.0.9",
    "@nestjs/websockets": "^7.0.9",
    "bull": "^3.13.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "mikro-orm": "^3.6.12",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "nest-access-control": "^2.0.1",
    "nest-schedule": "^0.6.4",
    "nestjs-mikro-orm": "^1.0.3",
    "p-queue": "^6.3.0",
    "passport": "^0.4.1",
    "passport-bnet": "^2.0.0",
    "passport-discord": "^0.1.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.9",
    "@types/bull": "^3.12.2",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.13.4",
    "@types/passport-discord": "^0.1.3",
    "@types/rc": "^1.1.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.5.3",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "tsc-watch": "^4.2.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
