{
  "name": "rbp-backend",
  "version": "0.0.1",
  "description": "API backend for the RBP gaming community.",
  "author": "Duckie",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"**/*.{ts,js,json,md,yml}\" --ignore-path .gitignore --write",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev-cli": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:prod": "node dist/main",
    "test": "jest --ci --color",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env CONFIG_PATH=../testing.env jest --config ./jest-e2e.json --ci --color -i"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.12",
    "@nestjs/core": "^6.10.12",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.12",
    "@nestjs/typeorm": "^6.2.0",
    "@types/lodash": "^4.14.149",
    "@types/rc": "^1.1.0",
    "bull": "^3.12.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "nest-access-control": "^2.0.1",
    "nest-bull": "^0.8.4",
    "nest-schedule": "^0.6.3",
    "passport": "^0.4.1",
    "passport-bnet": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.15.0",
    "promise.allsettled": "^1.0.2",
    "rc": "^1.2.8",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "slugify": "^1.3.6",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.11",
    "@types/bull": "^3.10.6",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/multer": "^1.3.10",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "24.7.1",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
