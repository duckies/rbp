{
  "name": "@rbp/server",
  "version": "1.0.0",
  "description": "API backend for the RBP gaming community.",
  "type": "module",
  "exports": "./dist/server/src/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"**/*.{ts,js,json,md,yml}\" --ignore-path .gitignore --write",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "start": "nest start",
    "dev-cli": "nest start --watch",
    "debug": "nest start --debug --watch",
    "dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --ci --color",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.e2e-config.js --color --runInBand --detectOpenHandles --forceExit",
    "mikro-orm": "mikro-orm"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@discordjs/opus": "0.5.3",
    "@golevelup/nestjs-discovery": "2.3.1",
    "@mikro-orm/cli": "4.5.7",
    "@mikro-orm/core": "4.5.7",
    "@mikro-orm/knex": "4.5.7",
    "@mikro-orm/migrations": "4.5.7",
    "@mikro-orm/nestjs": "4.2.0",
    "@mikro-orm/postgresql": "4.5.7",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs/bull": "0.4.0",
    "@nestjs/cli": "^7",
    "@nestjs/common": "^7",
    "@nestjs/config": "1.0.0",
    "@nestjs/core": "^7",
    "@nestjs/jwt": "^7",
    "@nestjs/passport": "^7",
    "@nestjs/platform-express": "^7",
    "@nestjs/platform-socket.io": "^7",
    "@nestjs/schedule": "1.0.0",
    "@nestjs/websockets": "^7",
    "@rbp/http": "1.0.0",
    "@sentry/node": "^6.9.0",
    "accesscontrol": "2.2.1",
    "bull": "3.22.9",
    "cache-manager": "3.4.4",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "discord.js": "12.5.3",
    "dotenv": "10.0.0",
    "ffmpeg-static": "4.4.0",
    "joi": "^17.4.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "multer": "1.4.2",
    "p-queue": "6.6.2",
    "passport": "0.4.1",
    "passport-bnet": "2.0.0",
    "passport-discord": "0.1.4",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "slugify": "1.5.3",
    "ytdl-core": "4.8.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7",
    "@types/bull": "3.15.2",
    "@types/express": "4.17.13",
    "@types/hapi__joi": "17.1.7",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.171",
    "@types/multer": "1.4.7",
    "@types/node": "16.3.1",
    "@types/passport-discord": "0.1.4",
    "@types/passport-oauth2": "1.4.10",
    "@types/rc": "1.1.0",
    "@types/socket.io": "3.0.2",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "27.0.6",
    "nest-raven": "^7.2.0",
    "prettier": "2.3.2",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.2.4"
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
