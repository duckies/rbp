{
  "name": "rbp-backend",
  "version": "0.0.1",
  "description": "API backend for the RBP gaming community.",
  "author": "Duckie",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"**/*.{ts,js,json,md,yml}\" --ignore-path .gitignore --write",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev-cli": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --ci --color",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.e2e-config.js --color --runInBand --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@hapi/joi": "^17.1.1",
    "@mikro-orm/cli": "^4.3.4",
    "@mikro-orm/core": "^4.3.4",
    "@mikro-orm/knex": "^4.3.4",
    "@mikro-orm/migrations": "^4.3.4",
    "@mikro-orm/nestjs": "^4.2.0",
    "@mikro-orm/postgresql": "^4.3.4",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/platform-socket.io": "^7.6.5",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/websockets": "^7.6.5",
    "@ntegral/nestjs-sentry": "^1.0.4",
    "@types/passport-oauth2": "^1.4.10",
    "accesscontrol": "^2.2.1",
    "bull": "^3.20.0",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7",
    "lodash": "^4.17.20",
    "mikro-orm": "^4.3.4",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "p-queue": "^6.6.2",
    "passport": "^0.4.1",
    "passport-bnet": "^2.0.0",
    "passport-discord": "^0.1.4",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "slugify": "^1.4.6",
    "source-map-support": "^0.5.19",
    "ytdl-core": "^4.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.5",
    "@types/bull": "^3.14.4",
    "@types/express": "^4.17.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.17",
    "@types/passport-discord": "^0.1.3",
    "@types/rc": "^1.1.0",
    "@types/socket.io": "^2.1.12",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
